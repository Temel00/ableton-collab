{
  "name": "gh-pages-nextjs",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "export": "next build && next export",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "check:format": "prettier --check \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "check:type": "tsc --project tsconfig.json --noEmit",
    "check:coverage": "type-coverage --strict --at-least 95"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.7.1",
    "@nextui-org/react": "^1.0.0-beta.13",
    "firebase": "^10.0.0",
    "next": "^13.4.9",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "react-date-picker": "^10.5.1",
    "react-datepicker": "^4.18.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-use-audio-player": "^2.1.1",
    "swr": "^2.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "17.0.14",
    "@types/react": "17.0.38",
    "eslint": "^8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "type-coverage": "^2.21.0",
    "typescript": "4.5.5"
  }
}
